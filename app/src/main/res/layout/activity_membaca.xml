<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Membaca" android:background="#665F5F">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <ImageView
                android:layout_width="match_parent"
                android:layout_height="110dp" tools:srcCompat="@drawable/main"
                android:id="@+id/imageView10" android:layout_marginTop="10dp"/>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="462dp" android:layout_marginTop="20dp" android:background="#3E4981">

            <TextView
                    android:text="MEMBACA"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView" android:textSize="35sp"
                    android:textStyle="bold" android:textColor="#F89C18" android:gravity="center"/>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="84dp" android:layout_marginTop="10dp">
                <ImageView
                        android:layout_width="250dp"
                        android:layout_height="wrap_content" app:srcCompat="@drawable/hurufa"
                        android:id="@+id/imageView21" android:layout_weight="1"/>
                <ImageView
                        android:layout_width="250dp"
                        android:layout_height="wrap_content" app:srcCompat="@drawable/hurufb"
                        android:id="@+id/imageView22" android:layout_weight="1"/>
                <ImageView
                        android:layout_width="250dp"
                        android:layout_height="wrap_content" app:srcCompat="@drawable/hurufc"
                        android:id="@+id/imageView23" android:layout_weight="1"/>
                <ImageView
                        android:layout_width="250dp"
                        android:layout_height="wrap_content" app:srcCompat="@drawable/hurufd"
                        android:id="@+id/imageView24" android:layout_weight="1"/>
                <ImageView
                        android:layout_width="250dp"
                        android:layout_height="wrap_content" app:srcCompat="@drawable/hurufe"
                        android:id="@+id/imageView25" android:layout_weight="1"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="84dp" android:layout_marginTop="10dp">
                <ImageView
                        android:layout_width="250dp"
                        android:layout_height="wrap_content" app:srcCompat="@drawable/huruff"
                        android:id="@+id/imageView26" android:layout_weight="1"/>
                <ImageView
                        android:layout_width="250dp"
                        android:layout_height="wrap_content" app:srcCompat="@drawable/hurufg"
                        android:id="@+id/imageView27" android:layout_weight="1"/>
                <ImageView
                        android:layout_width="250dp"
                        android:layout_height="wrap_content" app:srcCompat="@drawable/hurufh"
                        android:id="@+id/imageView28" android:layout_weight="1"/>
                <ImageView
                        android:layout_width="250dp"
                        android:layout_height="wrap_content" app:srcCompat="@drawable/hurufi"
                        android:id="@+id/imageView29" android:layout_weight="1"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@drawable/hurufj"
                        android:id="@+id/imageView30" android:layout_weight="1"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="84dp" android:layout_marginTop="10dp">
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="84dp" android:layout_marginTop="10dp">
            </LinearLayout>
        </LinearLayout>
        <ImageView
                android:layout_width="match_parent"
                android:layout_height="80dp" app:srcCompat="@android:drawable/ic_menu_revert"
                android:id="@+id/imageView31" android:layout_marginTop="20dp"/>
    </LinearLayout>
</RelativeLayout>