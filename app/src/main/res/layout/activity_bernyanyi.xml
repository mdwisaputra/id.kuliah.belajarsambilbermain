<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Bernyanyi" android:background="#665F5F">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <ImageView
                android:layout_width="match_parent"
                android:layout_height="110dp" tools:srcCompat="@drawable/main"
                android:id="@+id/imageView2" android:layout_marginTop="10dp"/>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="462dp" android:layout_marginTop="20dp" android:background="#3E4981">

            <TextView
                    android:text="BERNYANYI"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView5" android:textSize="35sp"
                    android:gravity="center" android:textStyle="bold" android:textColor="#F89C18"
                    android:layout_marginTop="20dp"/>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:layout_marginTop="20dp">
                <TextView
                        android:text="1. BALONKU"
                        android:layout_width="242dp"
                        android:layout_height="wrap_content" android:id="@+id/textView3" android:layout_weight="1"
                        android:textSize="30sp" android:layout_marginLeft="20dp" android:textColor="#F89C18"
                        android:textStyle="bold"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_media_play"
                        android:id="@+id/imageView11" android:layout_weight="1"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_media_pause"
                        android:id="@+id/imageView12" android:layout_weight="1"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="393dp"
                    android:layout_height="wrap_content" android:layout_marginTop="10dp"
                    android:layout_marginLeft="20dp">
                <TextView
                        android:text="2. BINTANG KECIL"
                        android:layout_width="239dp"
                        android:layout_height="wrap_content" android:id="@+id/textView6" android:layout_weight="1"
                        android:textSize="30sp"
                        android:textStyle="bold" android:textColor="#F89C18"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_media_play"
                        android:id="@+id/imageView13" android:layout_weight="1"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_media_pause"
                        android:id="@+id/imageView14" android:layout_weight="1"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:layout_marginLeft="20dp"
                    android:layout_marginTop="10dp">
                <TextView
                        android:text="3. NAIK KERETA API"
                        android:layout_width="240dp"
                        android:layout_height="wrap_content" android:id="@+id/textView7" android:layout_weight="1"
                        android:textSize="30sp" android:textStyle="bold" android:textColor="#F89C18"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_media_play"
                        android:id="@+id/imageView15" android:layout_weight="1" android:layout_gravity="center"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_media_pause"
                        android:id="@+id/imageView16" android:layout_weight="1" android:layout_gravity="center"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:layout_marginTop="10dp"
                    android:layout_marginLeft="20dp"/>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="388dp"
                    android:layout_height="wrap_content" android:layout_marginTop="10dp"
                    android:layout_marginLeft="20dp">
                <TextView
                        android:text="4. ABANG TUKANG BAKSO"
                        android:layout_width="240dp"
                        android:layout_height="wrap_content" android:id="@+id/textView9" android:layout_weight="1"
                        android:textSize="30sp" android:textStyle="bold" android:textColor="#F89C18"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_media_play"
                        android:id="@+id/imageView17" android:layout_weight="1" android:layout_gravity="center"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_media_pause"
                        android:id="@+id/imageView18" android:layout_weight="1" android:layout_gravity="center"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="390dp"
                    android:layout_height="wrap_content" android:layout_marginTop="10dp"
                    android:layout_marginLeft="20dp">
                <TextView
                        android:text="5. SATU SATU"
                        android:layout_width="242dp"
                        android:layout_height="wrap_content" android:id="@+id/textView10" android:layout_weight="1"
                        android:textSize="30sp" android:textStyle="bold" android:textColor="#F89C18"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_media_play"
                        android:id="@+id/imageView19" android:layout_weight="1"/>
                <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_media_pause"
                        android:id="@+id/imageView20" android:layout_weight="1"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView
                android:layout_width="match_parent"
                android:layout_height="80dp" app:srcCompat="@android:drawable/ic_menu_revert"
                android:id="@+id/imageView34" android:layout_marginTop="20dp"/>
    </LinearLayout>
</RelativeLayout>